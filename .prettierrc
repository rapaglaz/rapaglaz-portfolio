{
  "arrowParens": "avoid",
  "printWidth": 100,
  "singleAttributePerLine": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular"
      }
    }
  ],
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-jsdoc",
    "prettier-plugin-tailwindcss"
  ]
}
