{
  "name": "rapaglaz-portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --coverage --watch=false",
    "lint": "ng lint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "i18n:validate": "transloco-validator public/i18n/*.json",
    "i18n:find": "transloco-keys-manager find",
    "i18n:check": "pnpm run i18n:validate && pnpm run i18n:find",
    "preview": "pnpx http-server dist/rapaglaz-portfolio/browser -p 4233 -c-1",
    "e2e": "ng e2e",
    "e2e:ui": "ng e2e --ui",
    "clean": "rm -rf .angular && rm -rf node_modules",
    "clean:reinstall": "pnpm run clean && pnpm install"
  },
  "dependencies": {
    "@angular/cdk": "21.0.0",
    "@angular/common": "21.0.0",
    "@angular/compiler": "21.0.0",
    "@angular/core": "21.0.0",
    "@angular/forms": "21.0.0",
    "@angular/platform-browser": "21.0.0",
    "@angular/router": "21.0.0",
    "@fontsource/inter": "5.2.8",
    "@fontsource/manrope": "5.2.8",
    "@jsverse/transloco": "8.2.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.0.1",
    "@angular-eslint/eslint-plugin": "21.0.1",
    "@angular-eslint/eslint-plugin-template": "21.0.1",
    "@angular-eslint/template-parser": "21.0.1",
    "@angular/build": "21.0.0",
    "@angular/cli": "21.0.0",
    "@angular/compiler-cli": "21.0.0",
    "@eslint/js": "9.39.1",
    "@jsverse/transloco-keys-manager": "7.0.2",
    "@jsverse/transloco-utils": "8.2.0",
    "@jsverse/transloco-validator": "8.2.0",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "4.1.17",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.13",
    "angular-eslint": "21.0.1",
    "daisyui": "5.5.5",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "jsdom": "27.2.0",
    "playwright-ng-schematics": "21.0.1",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-jsdoc": "1.5.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.17",
    "type-fest": "5.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0",
    "vitest": "4.0.13"
  },
  "packageManager": "pnpm@10.23.0"
}
