{
  "name": "rapaglaz-portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --coverage --watch=false",
    "lint": "ng lint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "i18n:validate": "transloco-validator public/i18n/*.json",
    "i18n:find": "transloco-keys-manager find",
    "i18n:check": "pnpm run i18n:validate && pnpm run i18n:find",
    "preview": "http-server dist/rapaglaz-portfolio/browser -p 4233 -c-1",
    "e2e": "ng e2e",
    "e2e:ui": "ng e2e --ui",
    "e2e:ssg": "pnpm run build && E2E_MODE=ssg pnpm exec playwright test",
    "e2e:ssg:ui": "pnpm run build && E2E_MODE=ssg pnpm exec playwright test --ui",
    "clean": "rm -rf .angular && rm -rf node_modules",
    "clean:reinstall": "pnpm run clean && pnpm install",
    "prepare": "[ -n \"$CI\" ] || lefthook install"
  },
  "dependencies": {
    "@angular/cdk": "21.1.0",
    "@angular/common": "21.1.0",
    "@angular/compiler": "21.1.0",
    "@angular/core": "21.1.0",
    "@angular/forms": "21.1.0",
    "@angular/platform-browser": "21.1.0",
    "@angular/platform-server": "21.1.0",
    "@angular/router": "21.1.0",
    "@angular/ssr": "21.1.0",
    "@fontsource/inter": "5.2.8",
    "@fontsource/manrope": "5.2.8",
    "@jsverse/transloco": "8.2.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "21.1.0",
    "@angular/cli": "21.1.0",
    "@angular/compiler-cli": "21.1.0",
    "@axe-core/playwright": "4.11.0",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@eslint/js": "9.39.2",
    "@jsverse/transloco-keys-manager": "7.1.0",
    "@jsverse/transloco-utils": "8.2.1",
    "@jsverse/transloco-validator": "8.2.0",
    "@playwright/test": "1.57.0",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "24.10.9",
    "@vitest/coverage-v8": "4.0.17",
    "angular-eslint": "21.1.0",
    "daisyui": "5.5.14",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "http-server": "14.1.1",
    "jsdom": "27.4.0",
    "lefthook": "2.0.15",
    "lint-staged": "16.2.7",
    "playwright-ng-schematics": "21.0.3",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-jsdoc": "1.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "type-fest": "5.4.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vitest": "4.0.17"
  },
  "packageManager": "pnpm@10.28.1"
}
